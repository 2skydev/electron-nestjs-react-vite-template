{
  "name": "electron-nestjs-react-vite-template",
  "version": "1.0.0",
  "description": "electron + react + nestjs + vite template",
  "main": "./out/main/index.js",
  "license": "MIT",
  "author": {
    "email": "2skydev@gmail.com",
    "name": "2skydev"
  },
  "scripts": {
    "dev": "electron-vite dev -w",
    "build": "pnpm typecheck && pnpm biome:check && electron-vite build && electron-builder",
    "biome:check": "biome check ./src",
    "biome:check:fix": "biome check ./src --write",
    "typecheck": "pnpm typecheck:node && pnpm typecheck:web",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@nestjs/common": "^11.1.9",
    "@nestjs/core": "^11.1.9",
    "auto-launch": "^5.0.6",
    "electron-log": "^5.4.3",
    "electron-store": "^8",
    "electron-updater": "^6.6.2",
    "es-toolkit": "^1.42.0",
    "i18next": "^25.6.3",
    "jsonc-parser": "^3.3.1",
    "path-to-regexp": "^8.3.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "semver": "^7.7.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@hookform/resolvers": "^5.2.2",
    "@swc/core": "^1.15.3",
    "@tailwindcss/vite": "^4.1.17",
    "@types/auto-launch": "^5.0.5",
    "@types/node": "^22.15.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@types/semver": "^7.7.1",
    "@vitejs/plugin-react": "^5.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron": "^39.2.4",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.1",
    "lucide-react": "^0.555.0",
    "radix-ui": "^1.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.67.0",
    "react-i18next": "^16.3.5",
    "react-router-dom": "^7.9.6",
    "sonner": "^2.0.7",
    "swr": "^2.3.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "type-fest": "^5.2.0",
    "typescript": "^5.9.3",
    "vaul": "^1.1.2",
    "vite": "^7.2.6",
    "zod": "^4.1.13"
  }
}
